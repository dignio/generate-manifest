    "dotenv": "^16.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  },
  "bugs": {
    "url": "https://github.com/dignio/generate-manifest/issues"
  },
  "homepage": "https://github.com/dignio/generate-manifest#readme",
